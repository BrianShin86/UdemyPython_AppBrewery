# 🚨 Don't change the code below 👇
row1 = ["⬜️","⬜️","⬜️"]
row2 = ["⬜️","⬜️","⬜️"]
row3 = ["⬜️","⬜️","⬜️"]
map = [row1, row2, row3]
print(f"{row1}\n{row2}\n{row3}")
position = input("Where do you want to put the treasure? ")
# 🚨 Don't change the code above 👆

#Write your code below this row 👇
a = int(position [0])
b = int(position [1])

if a == 1 and b == 1:
  row1[0] = "X"
elif a == 1 and b == 2:
  row2[0] = "X"
elif a == 1 and b == 3:
  row3[0] = "X"
elif a == 2 and b == 1:
  row1[1] = "X"
elif a == 2 and b == 2:
  row2[1] = "X"
elif a == 2 and b == 3:
  row3[1] = "X"
elif a == 3 and b == 1:
  row1[2] = "X"
elif a == 3 and b == 2:
  row2[2] = "X"
elif a == 3 and b == 3:
  row3[2] = "X"

#Write your code above this row 👆

# 🚨 Don't change the code below 👇
print(f"{row1}\n{row2}\n{row3}")

# OR


# 🚨 Don't change the code below 👇
row1 = ["⬜️","⬜️","⬜️"]
row2 = ["⬜️","⬜️","⬜️"]
row3 = ["⬜️","⬜️","⬜️"]
map = [row1, row2, row3]
print(f"{row1}\n{row2}\n{row3}")
position = input("Where do you want to put the treasure? ")
# 🚨 Don't change the code above 👆

#Write your code below this row 👇

horizontal = int(position[0])
vertical =  int(position[1])

selected_row = map[vertical - 1]
selected_row[horizontal - 1] = "X"

#Write your code above this row 👆

# 🚨 Don't change the code below 👇
print(f"{row1}\n{row2}\n{row3}")
